# Generated by Ansible
LITELLM_MASTER_KEY={{ litellm_key }}
# Target LLM Key (Primary Choice during Deploy)
TARGET_LLM_KEY={{ llm_key }}
# OpenClaw Gateway Token
OPENCLAW_GATEWAY_TOKEN={{ openclaw_token }}

# Provider-Specific Keys (Can be manually updated on VPS)
GEMINI_API_KEY={{ final_gemini_key }}
OPENROUTER_API_KEY={{ final_openrouter_key }}
ANTHROPIC_API_KEY={{ final_anthropic_key }}
OPENAI_API_KEY={{ final_openai_key }}

# Force OpenClaw to use internal LiteLLM proxy for all Anthropic calls
ANTHROPIC_API_BASE=http://litellm:4000/v1
ANTHROPIC_API_KEY={{ litellm_key }}
